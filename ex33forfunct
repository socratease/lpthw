def loopfunction(max, increment):
  i=0
  numbers = []
  for i in range(0, max):
      print "At the top i is %d" % i
      numbers.append(i)
      
      i += increment
      print "Numbers now: ", numbers
      print "At the bottom i is %d" % i
  print "The numbers: "
  
  for num in numbers:
      print num
      
loopfunction(6,0)
